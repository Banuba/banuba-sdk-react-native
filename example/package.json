{
  "name": "@banuba/react-native-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build:android": "cd android && ./gradlew assembleDebug --no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a",
    "build:ios": "cd ios && xcodebuild -workspace ReactNativeExample.xcworkspace -scheme ReactNativeExample -configuration Debug -sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO"
  },
  "dependencies": {
    "react": "19.0.0",
    "react-native": "^0.79.0",
    "react-native-fs": "^2.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "^18.0.0",
    "@react-native-community/cli-platform-android": "^18.0.0",
    "@react-native-community/cli-platform-ios": "^18.0.0",
    "@react-native/babel-preset": "^0.79.0",
    "@react-native/eslint-config": "0.79.7",
    "@react-native/metro-config": "^0.79.0",
    "@react-native/typescript-config": "0.79.7",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "pod-install": "^0.1.0"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.1"
}
